// Schema do Banco de Dados - SQLite (Desenvolvimento Local)
// Para usar este schema localmente, renomeie para schema.prisma

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
  previewFeatures = []
  engineType = "binary"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

